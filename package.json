{
  "name": "react-ui-components",
  "version": "1.0.0",
  "main": "index.js",
  "repository": "https://vivekanand_s@bitbucket.org/vivekanand_s/react-ui-components.git",
  "author": "vivekanand_s <vivekanand.senthil@gmail.com>",
  "license": "MIT",
  "scripts": {
    "start": "webpack-dev-server --progress --colors --hot --inline --open",
    "build": "webpack --mode production",
    "test": "set BABEL_ENV=development&& jest --watch",
    "pdn": "set BABEL_ENV=production&& jest --watch",
    "dev": "npm-run-all --parallel start test",
    "production": "set BABEL_ENV=production&&webpack-dev-server --progress --colors --hot --inline --open",
    "docz:dev": "docz dev",
    "docz:build": "docz build",
    "docz:serve": "docz build && docz serve"
  },
  "jest": {
    "snapshotSerializers": [
      "enzyme-to-json/serializer"
    ],
    "setupFiles": [
      "./src/setupTests.js"
    ],
    "testPathIgnorePatterns": [
      "/node_modules",
      ".docz"
    ],
    "moduleNameMapper": {
      "^@utils(.*)$": "<rootDir>/src/utils$1",
      "^@components(.*)$": "<rootDir>/src/components$1",
      "^@config(.*)$": "<rootDir>/src/config$1"
    }
  },
  "dependencies": {
    "@babel/polyfill": "^7.8.7",
    "prop-types": "^15.7.2",
    "react": "^16.13.0",
    "react-dom": "^16.13.0",
    "react-transition-group": "^4.3.0"
  },
  "devDependencies": {
    "@babel/cli": "^7.8.4",
    "@babel/core": "^7.8.7",
    "@babel/plugin-proposal-object-rest-spread": "^7.8.3",
    "@babel/preset-env": "^7.8.7",
    "@babel/preset-react": "^7.8.3",
    "@types/jest": "^25.1.4",
    "@types/styled-components": "^5.0.1",
    "babel-jest": "^25.3.0",
    "babel-loader": "^8.0.6",
    "babel-plugin-react-remove-properties": "^0.3.0",
    "check-prop-types": "^1.1.2",
    "docz": "^2.3.1",
    "enzyme": "^3.11.0",
    "enzyme-adapter-react-16": "^1.15.2",
    "enzyme-to-json": "^3.4.4",
    "html-webpack-plugin": "^3.2.0",
    "jest": "^25.1.0",
    "npm-run-all": "^4.1.5",
    "styled-components": "^5.0.1",
    "webpack": "^4.42.0",
    "webpack-cli": "^3.3.11",
    "webpack-dev-server": "^3.10.3"
  }
}
